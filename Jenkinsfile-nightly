pipeline {
    agent any
    stages {
        stage ('Build Docker Image') {
          agent any
          steps {
            sh 'docker build -f Jenkins_Jobs_Pipelines_CICD_and_DevOps_for_Beginners\learn-jenkins-app\ci\Dockerfile-playwright -t my-playwright .'
            sh 'docker build -f Jenkins_Jobs_Pipelines_CICD_and_DevOps_for_Beginners\learn-jenkins-app\ci\Dockerfile-aws -t my-aws-cli .'
          }
        }
    }
}
